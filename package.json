{
  "name": "stateless-maybe-js",
  "description": "A stateless implementation of the maybe monad",
  "version": "1.0.0",
  "author": "Emiliano Bovetti",
  "license": "GPL-3.0",
  "keywords": [
    "maybe",
    "option",
    "monad",
    "optional",
    "nullable",
    "null",
    "undefined",
    "functional",
    "fp",
    "typescript"
  ],
  "main": "./src/maybe.js",
  "types": "./src/maybe.d.ts",
  "scripts": {
    "test": "./node_modules/.bin/mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emilianobovetti/stateless-maybe-js.git"
  },
  "bugs": {
    "url": "https://github.com/emilianobovetti/stateless-maybe-js/issues"
  },
  "release": {
    "branch": "master",
    "prepare": [
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "dist/*.js"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "@semantic-release/github"
    ]
  },
  "devDependencies": {
    "@semantic-release/git": "^7.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.1.0",
    "jsverify": "^0.8.3",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "semantic-release": "^15.8.1",
    "should": "^13.2.1",
    "should-sinon": "^0.0.6",
    "sinon": "^6.1.3",
    "uglify-js": "^3.4.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
